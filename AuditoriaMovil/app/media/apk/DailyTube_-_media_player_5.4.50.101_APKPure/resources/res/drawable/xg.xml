<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:gravity="center_vertical" android:id="@android:id/background" android:height="2dp">
        <shape>
            <solid android:color="#4dd8d8d8"/>
            <size android:height="2dp"/>
        </shape>
    </item>
    <item android:gravity="center_vertical" android:id="@android:id/progress" android:height="2dp">
        <clip>
            <shape>
                <gradient android:startColor="#ff8d53f4" android:endColor="#808d53f4" android:angle="0"/>
                <size android:height="2dp"/>
            </shape>
        </clip>
    </item>
</layer-list>
