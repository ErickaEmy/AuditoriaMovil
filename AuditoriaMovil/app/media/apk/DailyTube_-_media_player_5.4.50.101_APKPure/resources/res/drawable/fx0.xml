<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <layer-list>
            <item android:gravity="center" android:right="3dp">
                <shape android:shape="rectangle">
                    <stroke android:width="0.5dp" android:color="#0a000000"/>
                    <size android:height="11dp" android:width="11dp"/>
                    <corners android:radius="6dp"/>
                    <solid android:color="#ffffffff"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="false">
        <layer-list>
            <item android:gravity="center" android:left="3dp">
                <shape android:shape="rectangle">
                    <stroke android:width="0.5dp" android:color="#0a000000"/>
                    <size android:height="11dp" android:width="11dp"/>
                    <corners android:radius="6dp"/>
                    <solid android:color="#b3ffffff"/>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
