    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#00000000" android:endColor="#08000000" android:angle="90"/>
            <corners android:radius="42dp"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="3dp">
