<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <corners android:radius="10dp"/>
            <gradient android:startColor="#ff915cdb" android:endColor="#ff915cdb" android:angle="270" android:type="linear"/>
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape>
            <corners android:radius="10dp"/>
            <gradient android:startColor="#ff915cdb" android:endColor="#ff915cdb" android:angle="270" android:type="linear" android:centerColor="#ff915cdb"/>
        </shape>
    </item>
</layer-list>
