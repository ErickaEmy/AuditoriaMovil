<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <layer-list>
            <item android:gravity="center" android:left="4dp" android:right="0dp">
                <shape android:shape="oval">
                    <solid android:color="@color/ju"/>
                    <size android:height="8dp" android:width="8dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
    <item android:state_checked="true">
        <layer-list>
            <item android:gravity="center" android:left="0dp" android:right="4dp">
                <shape android:shape="oval">
                    <solid android:color="@color/ju"/>
                    <size android:height="8dp" android:width="8dp"/>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>
