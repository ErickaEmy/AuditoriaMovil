                <TextView android:textSize="15sp" android:textColor="?attr/fdq" android:id="@+id/timerTextView" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="36dp"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical" android:id="@+id/llSound" android:layout_width="match_parent" android:layout_height="wrap_content">
                <ImageView android:id="@+id/ivSound" android:layout_width="24dp" android:layout_height="24dp" android:layout_marginTop="10dp" android:layout_marginBottom="10dp" android:layout_marginStart="26dp" app:srcCompat="@drawable/frm"/>
                <TextView android:textSize="15sp" android:textColor="?attr/fdq" android:id="@+id/switchMute" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="36dp"/>
            </LinearLayout>
            <LinearLayout android:gravity="center_vertical" android:id="@+id/llCast" android:visibility="gone" android:layout_width="match_parent" android:layout_height="wrap_content">
                <ImageView android:id="@+id/ivCast" android:layout_width="24dp" android:layout_height="24dp" android:layout_marginTop="10dp" android:layout_marginBottom="10dp" android:layout_marginStart="26dp" app:srcCompat="@drawable/frg"/>
