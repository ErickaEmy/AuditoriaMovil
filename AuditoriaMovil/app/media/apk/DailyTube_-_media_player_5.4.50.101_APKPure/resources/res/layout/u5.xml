            </LinearLayout>
            <LinearLayout android:gravity="center_vertical" android:id="@+id/llBrowser" android:layout_width="match_parent" android:layout_height="wrap_content">
                <ImageView android:id="@+id/ivBrowser" android:layout_width="24dp" android:layout_height="24dp" android:layout_marginTop="10dp" android:layout_marginBottom="10dp" android:layout_marginStart="26dp" app:srcCompat="@drawable/frk"/>
                <TextView android:textSize="15sp" android:textColor="?attr/fdq" android:id="@+id/openInBrowser" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginStart="36dp"/>
